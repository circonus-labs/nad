#!/usr/sbin/dtrace -s
#pragma D option quiet
fq*:::message-deliver
{
    @l[strjoin(substr(args[0]->pretty,0,index(args[0]->pretty,"/")), "`latency_ns")] = llquantize(args[2]->latency,10,0,9,100);
    @l["payload_len"] = llquantize(args[2]->payload_len,10,0,9,100);
}
tick-1sec
{
    printf(">START\n");
    printa("=%s%@d\n", @l);
    printf(">END\n");

    trunc(@l);
}
